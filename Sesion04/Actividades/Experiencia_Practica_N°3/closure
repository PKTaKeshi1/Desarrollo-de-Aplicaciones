<head>
  <meta charset="UTF-8">
  <title>Closure</title>
</head>
<body>
  <h1>Ejemplo de Closure</h1>
  <script>
    function makeFunc() {
      const name = "Mozilla";
      function displayName() {
        console.log(name);
      }
      return displayName;
    }

    const myFunc = makeFunc();
    myFunc();
  </script>
</body>
</html>
